# Rent Management App Environment Configuration
# Copy this file to .env and modify as needed

# Application Settings
NODE_ENV=production
PORT=9999
DATA_PATH=/app/data

# Logging
LOG_LEVEL=info
LOG_FILE=/app/logs/app.log

# Data Settings
CSV_ENCODING=utf8
BACKUP_ENABLED=true
BACKUP_INTERVAL=24h

# Security (Optional)
# JWT_SECRET=your-secret-key-here
# SESSION_SECRET=your-session-secret-here

# Performance
MAX_MEMORY=512m
NODE_OPTIONS=--max-old-space-size=512
